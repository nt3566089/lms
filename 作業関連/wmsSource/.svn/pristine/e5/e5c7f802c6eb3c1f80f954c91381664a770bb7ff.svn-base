package com.oneslogi.ht.base.dto;

/**
 * パスワードの情報を保持します。
 * @author at.inoue.hx
 *
 */
public class HandyLoginPasswordDto extends HandyBaseDto {
	// パスワードの入力が必須か？
	private boolean needInput;
	// 入力されたパスワード
	private String pass;
	
	public HandyLoginPasswordDto(String pass) {
		this.pass = pass;
		this.needInput = true;
	}
	
	public boolean isNeedInput() {
		return needInput;
	}
	public void setNeedInput(boolean needInput) {
		this.needInput = needInput;
	}
	public String getPass() {
		return pass;
	}
	public void setPass(String pass) {
		this.pass = pass;
	}
}
