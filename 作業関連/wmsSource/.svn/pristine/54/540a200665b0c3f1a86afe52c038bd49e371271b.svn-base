package com.oneslogi.ht.base.dto;

/**
 * HywayBrowserから取得できるパラメータを保持します。
 * @author hi.watanabe.mp
 *
 */
public class HywayDefaultParamDto {

	private String uniquePacketId;
	private String htLocalId;
	private String htUniqueId;
	private String groupId;
	private String browserVersion;
	private String printResult;
	private String htLang;
	
	/**
	 * 通信パケットIDを取得します。
	 * @return 通信パケットID
	 */
	public String getUniquePacketId() {
		return uniquePacketId;
	}

	public void setUniquePacketId(String uniquePacketId) {
		this.uniquePacketId = uniquePacketId;
	}

	/**
	 * 端末のローカルIDを取得します。<br>
	 * ローカルIDは、端末のIPアドレスです。<br>
	 * @return ローカルID
	 */
	public String getHtLocalId() {
		return htLocalId;
	}

	public void setHtLocalId(String htLocalId) {
		this.htLocalId = htLocalId;
	}

	/**
	 * 端末のユニークIDを取得します。<br>
	 * ユニークIDは、上2ケタを除いた値が端末のMACアドレスになります。<br>
	 * @return ユニークID
	 */
	public String getHtUniqueId() {
		return htUniqueId;
	}

	public void setHtUniqueId(String htUniqueId) {
		this.htUniqueId = htUniqueId;
	}

	/**
	 * 端末のグループIDを取得します。<br>
	 * @return グループID
	 */
	public String getGroupId() {
		return groupId;
	}

	public void setGroupId(String groupId) {
		this.groupId = groupId;
	}

	/**
	 * HywayBrowserのバージョンを取得します。<br>
	 * @return バージョン
	 */
	public String getBrowserVersion() {
		return browserVersion;
	}

	public void setBrowserVersion(String browserVersion) {
		this.browserVersion = browserVersion;
	}

	/**
	 * 印刷結果を取得します。<br>
	 * @return 印刷結果
	 */
	public String getPrintResult() {
		return printResult;
	}

	public void setPrintResult(String printResult) {
		this.printResult = printResult;
	}

	/**
	 * 端末の言語を取得します。<br>
	 * @return 言語
	 */
	public String getHtLang() {
		return htLang;
	}

	public void setHtLang(String htLang) {
		this.htLang = htLang;
	}
	

}
